#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lionheart/clawdbot/node_modules/.pnpm/openclaw@2026.2.13_@napi-rs+canvas@0.1.92_@types+express@5.0.6_audio-decode@2.2.3_hono@_eab0b7fa59366636daca68a6adaecb0b/node_modules/openclaw/node_modules:/Users/lionheart/clawdbot/node_modules/.pnpm/openclaw@2026.2.13_@napi-rs+canvas@0.1.92_@types+express@5.0.6_audio-decode@2.2.3_hono@_eab0b7fa59366636daca68a6adaecb0b/node_modules:/Users/lionheart/clawdbot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lionheart/clawdbot/node_modules/.pnpm/openclaw@2026.2.13_@napi-rs+canvas@0.1.92_@types+express@5.0.6_audio-decode@2.2.3_hono@_eab0b7fa59366636daca68a6adaecb0b/node_modules/openclaw/node_modules:/Users/lionheart/clawdbot/node_modules/.pnpm/openclaw@2026.2.13_@napi-rs+canvas@0.1.92_@types+express@5.0.6_audio-decode@2.2.3_hono@_eab0b7fa59366636daca68a6adaecb0b/node_modules:/Users/lionheart/clawdbot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openclaw/openclaw.mjs" "$@"
else
  exec node  "$basedir/../openclaw/openclaw.mjs" "$@"
fi
